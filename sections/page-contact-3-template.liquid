{%- include 'page-contact-snippet' -%}
{% schema %}
{
   "name":"Contact Page",
   "max_blocks":6,
   "settings":[
      {
         "type":"checkbox",
         "id":"show_breabcrumb",
         "label":"Show Breadcrumb",
         "default":true
      },
      {
         "type":"image_picker",
         "id":"breadcrumb_image",
         "label":"Breadcrumb image"
      },
      {
         "type":"header",
         "content":"LAYOUT"
      },
      {
         "type"  :"checkbox",
         "id"    :"boxed_layout",
         "label" :"Boxed Layout"
      },
      {
         "type":"color",
         "id":"background_color",
         "label":"Background color"
      },
      {
         "type":"header",
         "content":"Heading content",
         "info":"Blank to hide."
      },
      {
         "type":"text",
         "id":"heading_text",
         "label":"Heading"
      },
      {
         "type":"textarea",
         "id":"subheading_text",
         "label":"Subheading"
      }
   ],
   "blocks":[
      {
         "type":"text",
         "name":"Contact Group",
         "settings":[
            {
                "type"  :"select",
                "id"    :"width",
                "label" :"Width",
                "default":"6",
                "options":[
                  {"value": "6","label": "50%"},
                  {"value": "12","label": "100%"}
                ]
            },
            {
               "type"  :"richtext",
               "id"    :"text_1",
               "label" :"Group text 1"
            },
            {
               "type"  :"richtext",
               "id"    :"text_2",
               "label" :"Group text 2"
            },
            {
               "type"  :"richtext",
               "id"    :"text_3",
               "label" :"Group text 3"
            },
            {
               "type"  :"richtext",
               "id"    :"text_4",
               "label" :"Group text 4"
            }
         ]
      },
      {
         "type":"form",
         "name":"Form contact",
         "limit":1,
         "settings":[
            {
                "type"  :"select",
                "id"    :"width",
                "label" :"Width",
                "default":"6",
                "options":[
                  {"value": "6","label": "50%"},
                  {"value": "12","label": "100%"}
                ]
            },
            {
               "type":"text",
               "id":"heading",
               "label":"Title",
               "info":"Blank to hide."
            },
            {
               "type":"textarea",
               "id":"subheading",
               "label":"Subtitle",
               "info":"Blank to hide."
            },
            {
               "type":"select",
               "id":"head_align",
               "label":"Heading alignment",
               "default":"left",
               "options":[
                  {"value":"left","label":"Left"},
                  {"value":"center","label":"Center"},
                  {"value": "right","label": "Right"}
               ]
            },
            {
               "type":"select",
               "id":"form_align",
               "label":"Form content alignment",
               "default":"left",
               "options":[
                  {"value":"left","label":"Left"},
                  {"value":"center","label":"Center"},
                  {"value": "right","label": "Right"}
               ]
            },
            {
               "type":"select",
               "id":"btn_align",
               "label":"Submit button alignment",
               "default":"left",
               "options":[
                  {"value":"left","label":"Left"},
                  {"value":"center","label":"Center"},
                  {"value":"right","label":"Right"}
               ]
            }
          ]
      },
