{% assign modules = 'module_profile,module_address,module_order,module_topOrder,module_wishlist,module_recentlyView,module_storeCredit,module_howToEarn,module_howToSpend,module_fromAdmin,module_referFriend,module_changePassword' | split: ',' %}

{% case flits_main_component %}
	
	{%- when 'comp_greeting' -%}
		<h1 class="flits-greeting-text">
			<span class="flits-greeting-title"></span> <span class="flits-user-name">{{ customer.first_name | strip }}</span>
		</h1>
	{%- when 'comp_menuNav' -%}
		<div class="flits-menu-nav">
			<div class="flits-navigation-header flits-with-shadow">
				<div class="flits-user-avatar" data-flits-name="{{ customer.first_name | slice: 0 | strip }}{{ customer.last_name | slice: 0 | strip }}">
				</div>
				<div class="flits-user-box">
					<div class="flits-d-flex">
						<h4 class="flits-user-name" data-tippy-content="{{ customer.name | strip }}">{{ customer.name | strip }}</h4>
					</div>
					<div class="flits-d-flex flits-align-items-center">
						<svg class="flits-svg-clock">
							<g>
								<circle class="flits-clock-circle" cx="50%" cy="50%" r="45%"></circle>
							</g>
							<g class="flits-clock-hande">
								<line x1="50%" y1="50%" x2="50%" y2="25%" id="flits-hourhand">
								</line>
								<line x1="50%" y1="50%" x2="50%" y2="15%" id="flits-minutehand">
								</line>
								<line x1="50%" y1="50%" x2="50%" y2="12%" id="flits-secondhand">
								</line>
							</g>
							<circle class="flits-clock-hand-center" cx="50%" cy="50%" r="3%"></circle>
						</svg>
						<p class="flits-current-time"></p>
					</div>
				</div>
			</div>
			<div class="flits-menus-list-box">
				{% render 'flits_capture_code' with 'component',template:'comp_menuList' attr:true %}
			</div>
		</div>
	{%- when 'comp_menuList' -%}
		<div class="flits-menu-items">
			{% render 'flits_capture_code' with 'component',template:'comp_menuItems' attr:attr%}
		</div>
	{%- when 'comp_menuItems' -%}
		{% assign is_divider_on = attr %}

		{% liquid 
			assign profileIcon= '<svg xmlns="http://www.w3.org/2000/svg" id="Layer_1" data-name="Layer 1" viewBox="0 0 16 15.94"><path id="Layer_2" data-name="Layer 2" d="M16,16h-.78A7.22,7.22,0,1,0,.78,16H0A8,8,0,0,1,7.93,7.93H8a8,8,0,0,1,8,8ZM11.52,3.55A3.52,3.52,0,1,0,8,7.1,3.52,3.52,0,0,0,11.52,3.55Zm-.79,0A2.73,2.73,0,1,1,8,.79,2.73,2.73,0,0,1,10.73,3.55Z" transform="translate(0 -0.06)" /></svg>'
			render 'flits_capture_code' with 'component',template:'comp_menuItem',href:"#profile",text:'My Profile',attr:'data-flits-lang="navigation.profile" data-flits-lang-default="My Profile"', img:profileIcon
		%}
		{% liquid 
