			render 'flits_capture_code' with 'component',template:'comp_menuItem',href:"#address",text:'Delivery Address',attr:'data-flits-lang="navigation.delivery_address" data-flits-lang-default="Delivery Address"', img:addressIcon, badge:'<span class="flits-menu-badge flits-badge flits-address-badge flits-hide">0</span>'
